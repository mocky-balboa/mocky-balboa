{
  "name": "@mocky-balboa/example-sveltekit-2-csr",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:tests": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:cypress": "pnpm cypress run",
    "test:playwright": "pnpm playwright test",
    "start:tests": "mocky-balboa-sveltekit -h 0.0.0.0",
    "start:tests-dev": "pnpm dev --port 3000"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@mocky-balboa/cypress": "workspace:*",
    "@mocky-balboa/playwright": "workspace:*",
    "@mocky-balboa/sveltekit": "workspace:*",
    "@sveltejs/adapter-node": "^5.3.1",
    "@sveltejs/kit": "^2.37.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.3",
    "svelte": "^5.38.6",
    "svelte-check": "^4.3.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.4",
    "@playwright/test": "^1.54.2",
    "cypress": "^15.0.0",
    "playwright": "^1.54.2"
  }
