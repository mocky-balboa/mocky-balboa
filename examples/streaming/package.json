{
	"name": "@mocky-balboa/example-streaming",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"build": "pnpm build:client && pnpm build:server",
		"build:client": "esbuild client.bundle.mjs --bundle --minify --outfile=public/client.bundle.js --log-level=error",
		"build:server": "swc src --out-dir dist -C module.type=es6 --strip-leading-paths",
		"build:watch": "concurrently \"pnpm run build:client --watch\" \"pnpm run build:server --watch\"",
		"build:tests": "pnpm build",
		"start": "node dist/server.js",
		"start:tests": "pnpm start",
		"start:tests-dev": "pnpm start",
		"test:cypress": "pnpm cypress run",
		"test:playwright": "pnpm playwright test"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.14.0",
	"dependencies": {
		"@microsoft/fetch-event-source": "^2.0.1",
		"express": "catalog:"
	},
	"devDependencies": {
		"@mocky-balboa/cypress": "workspace:*",
		"@mocky-balboa/playwright": "workspace:*",
		"@mocky-balboa/server": "workspace:*",
		"@playwright/test": "^1.55.0",
		"@swc/cli": "catalog:",
		"@swc/core": "catalog:",
		"@types/express": "catalog:",
		"@types/node": "^24",
		"concurrently": "catalog:",
		"cypress": "^15.2.0",
		"esbuild": "catalog:",
		"playwright": "^1.55.0",
		"typescript": "^5.8.3"
	}
}
