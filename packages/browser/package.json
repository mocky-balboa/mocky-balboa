{
  "name": "@mocky-balboa/browser",
  "version": "0.0.0",
  "description": "Browser integration for Mocky Balboa.",
  "type": "module",
  "homepage": "https://docs.mockybalboa.com",
  "bugs": {
    "url": "https://github.com/mocky-balboa/mocky-balboa/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mocky-balboa/mocky-balboa.git",
    "directory": "packages/browser"
  },
  "keywords": [],
  "author": {
    "name": "Lewis Nelson",
    "url": "https://lewnelson.com"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "main": "./dist/event-source-stub.js",
  "types": "./dist/types/playwright.d.ts",
  "exports": {
    "./event-source-stub": {
      "import": "./dist/event-source-stub.js",
      "require": "./dist/event-source-stub.js",
      "types": "./dist/types/event-source-stub.d.ts"
    },
    "./fetch-stub": {
      "import": "./dist/fetch-stub.js",
      "require": "./dist/fetch-stub.js",
      "types": "./dist/types/fetch-stub.d.ts"
    }
  },
  "scripts": {
    "build:event-source-stub": "esbuild src/event-source-stub.ts --bundle --minify --outfile=dist/event-source-stub.js --log-level=error",
    "build:fetch-stub": "esbuild src/fetch-stub.ts --bundle --minify --outfile=dist/fetch-stub.js --log-level=error",
    "build": "pnpm clean && pnpm run build:event-source-stub && pnpm run build:fetch-stub && pnpm run build:types && pnpm run post:build",
    "build:watch": "concurrently \"pnpm run build:event-source-stub --watch\" \"pnpm run build:fetch-stub --watch\" \"pnpm run build:types --watch\" \"pnpm run post:build:watch\"",
    "build:types": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "post:build": "node scripts/patch-dist.cjs",
    "post:build:watch": "chokidar \"dist/*.js\" -c \"pnpm run post:build\"",
    "clean": "rimraf dist",
    "release": "semantic-release",
    "test": "vitest"
  },
  "dependencies": {
    "@mocky-balboa/client": "workspace:*",
    "@mocky-balboa/logger": "workspace:*",
    "@mocky-balboa/shared-config": "workspace:*"
  },
  "devDependencies": {
    "chokidar-cli": "catalog:",
    "@types/node": "catalog:",
    "concurrently": "catalog:",
    "esbuild": "0.25.10",
    "rimraf": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
