{
  "name": "@mocky-balboa/react-router",
  "version": "1.1.3",
  "description": "Mocky Balboa integration for React Router.",
  "type": "module",
  "homepage": "https://docs.mockybalboa.com",
  "bugs": {
    "url": "https://github.com/mocky-balboa/mocky-balboa/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mocky-balboa/mocky-balboa.git",
    "directory": "packages/react-router"
  },
  "keywords": [
    "react-router",
    "mocky",
    "balboa",
    "mocking",
    "network",
    "testing",
    "tests",
    "integration",
    "server"
  ],
  "author": {
    "name": "Lewis Nelson",
    "url": "https://lewnelson.com"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.14.0",
  "main": "./dist/cjs/react-router.cjs",
  "bin": {
    "mocky-balboa-react-router": "./dist/esm/cli.js"
  },
  "types": "./dist/types/react-router.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/react-router.js",
      "require": "./dist/cjs/react-router.cjs",
      "types": "./dist/types/react-router.d.ts"
    }
  },
  "scripts": {
    "build:esm": "swc src --out-dir dist/esm -C module.type=es6 --strip-leading-paths",
    "build:cjs": "swc src --out-dir dist/cjs -C module.type=commonjs --strip-leading-paths --out-file-extension cjs",
    "build": "pnpm clean && pnpm run build:esm && pnpm run build:cjs && pnpm run build:types && pnpm run post:build",
    "build:types": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "build:watch": "concurrently \"pnpm run build:esm --watch\" \"pnpm run build:cjs --watch\" \"pnpm run build:types --watch\" \"pnpm run post:build:watch\"",
    "post:build": "pnpm --workspace-root package-post-cjs-build $(pwd)",
    "post:build:watch": "chokidar \"dist/cjs/**/*.cjs\" -c \"pnpm run post:build\"",
    "clean": "rimraf dist",
    "release": "semantic-release",
    "test": "echo \"No tests\""
  },
  "dependencies": {
    "@mocky-balboa/cli-utils": "workspace:*",
    "@mocky-balboa/logger": "workspace:*",
    "@mocky-balboa/vite": "workspace:*",
    "@react-router/express": "^7.8.2"
  },
  "devDependencies": {
    "@swc/cli": "catalog:",
    "@swc/core": "catalog:",
    "chokidar-cli": "catalog:",
    "@types/node": "catalog:",
    "concurrently": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:"
  }
}
